  "scripts": {
    "build:next": "production=true next build",
    "build": "npm-run-all -p build:next build:server",
    "build:watch": "next dev --port=3001",
    "serve": "node --enable-source-maps dist/server.js",
    "serve:dev": "wait-on dist/server.js && NODE_OPTIONS=--enable-source-maps nodemon dist/server.js",
    "build:server": "webpack",
    "build:server:watch": "webpack --watch --progress",
    "dev": "npm-run-all -s clean -p build:watch build:server:watch serve:dev",
    "clean": "rimraf dist",
    "gen": "scaffdog generate",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "init:seeds": "ts-node-transpile-only scripts/init.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@next/bundle-analyzer": "^14.1.0",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-loader": "^9.1.3",
    "clsx": "^2.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sort": "^2.11.0",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "scaffdog": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@fastify/compress": "^7.0.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/static": "^7.0.1",
    "dayjs": "^1.11.10",
    "fastify": "^4.26.1",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.22.0",
    "regenerator-runtime": "^0.14.1",
    "sqlite3": "^5.1.7",
